@model IEnumerable<RockstarsManagementSquad.Models.RockstarViewModel>
@{
    ViewData["Title"] = "Squads";
}

<div class="text-center">
    <form method="post" action="">
        <input type="text" name="id" placeholder="zeg even" />
        <input type="text" name="address" placeholder="wat je" />
        <input type="tel"  name="telNr" placeholder="hier wilt" />
        <input type="submit" name="submit" value="Submit" />
    </form>
</div>

<div>
    <br/>
    <br/>
    <table class="table">
        <tdead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Email</th>
                <th>RoleID</th>
                <th>SquadID</th>
            </tr>
        </tdead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.username</td>
                <td>@item.email</td>
                <td>@item.roleid</td>
                <td>@item.squadid</td>
            </tr>
        }
        </tbody>
    </table>
</div>