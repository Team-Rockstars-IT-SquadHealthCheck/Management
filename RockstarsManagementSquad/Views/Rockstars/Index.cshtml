@model IEnumerable<RockstarsManagementSquad.Models.RockstarViewModel>
@{
    ViewData["Title"] = "Rockstars";
}

<div class="text-center">
    <form asp-controller="Rockstars" method="post" asp-action="Create">
        <input type="text" name="username" placeholder="UserName" />
        <input type="text" name="email" placeholder="Email" />
        <input type="text" name="password" placeholder="Password" />
        <input type="text" name="roleid" placeholder="RoleID" />
        <input type="text" name="squadid" placeholder="SquadID" />
        <input type="submit" name="submit" value="Create" />
    </form>
</div>

<div>
    <br/>
    <br/>
    <table class="table">
        <tdead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Email</th>
                <th>RoleID</th>
                <th>SquadID</th>
                <th>url</th>
            </tr>
        </tdead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.username</td>
                <td>@item.email</td>
                <td>@item.roleid</td>
                <td>@item.squadid</td>
                <td>@item.url</td>
            </tr>
        }
        </tbody>
    </table>
</div>