@model IEnumerable<RockstarsManagementSquad.Models.RockstarViewModel>
@{
    ViewData["Title"] = "Squads";
}

<a href="#" class="btn btn-primary" data-toggle="modal" data-target="#userModal">Add User</a>


<div>
    <br/>
    <br/>
    <table class="table">
        <tdead>
            <tr>
                <th>ID</th>
                <th>Username</th>
                <th>Email</th>
                <th>RoleID</th>
                <th>SquadID</th>
            </tr>
        </tdead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.username</td>
                <td>@item.email</td>
                <td>@item.roleid</td>
                <td>@item.squadid</td>
            </tr>
        }
        </tbody>
    </table>
</div>